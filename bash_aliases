# -*-Shell-Script-*-
alias ll='ls -lh'
alias la='ls -lah'
alias l='ls -CF'

alias chrome='open -a Google\ Chrome'
alias ff='open -a Firefox'

alias gitg='git log --pretty=format:"%h|%an|%s" --graph'
alias cached='git diff --cached'
alias st='git status'
alias status='git status'

alias emacs='emacs-snapshot'
